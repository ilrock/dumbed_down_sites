{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "react/prop-types": ["error", {"skipUndeclared": true}],
      "no-unused-vars": ["error", { "args": "none" }]
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}